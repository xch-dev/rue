fun main(value: Int) -> Int {
    fun inner_1(num: Int) -> Int {
        outer_1() * value + num
    }

    fun inner_2(num: Int) -> Int {
        outer_2(num, inner_1)
    }

    outer_1() + inner_1(inner_2(value)) * outer_2(value, inner_2)
}

fun outer_1() -> Int {
    96
}

fun outer_2(num: Int, closure: fun(Int) -> Int) -> Int {
    num * closure(num)
}
